    <name>ImgurWatchTest</name>
    <description>
        An unofficial Imgur application for viewing random images on a website or mobile app with optional Apple Watch support.
    </description>
    <author email="catlin.sean@gmail.com" href="http://cowdino.com">
        Sean Catlin via cowdino
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="StatusBarOverlaysWebView" value="false" />
    <preference name="StatusBarBackgroundColor" value="#D84315" />
    <preference name="StatusBarStyle" value="lightcontent" />
    <feature name="StatusBar">
        <param name="ios-package" value="CDVStatusBar" />
        <param name="onload" value="true" />
    </feature>
    <preference name="StatusBarOverlaysWebView" value="true" />
    <feature name="AppleWatch">
        <param name="ios-package" value="AppleWatch" />
        <param name="onload" value="true" />
    </feature>
</widget>
